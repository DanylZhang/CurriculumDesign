                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <telerik:RadButton Name="btnStart" Content="开始" Grid.Column="1" HorizontalAlignment="Center" Height="51" Margin="20" Grid.Row="1" VerticalAlignment="Center" Width="115" Click="btnStart_Click"/>
        </Grid>
    </Grid>
</Window>